import com.allen.core.generic.GenericService;
import com.allen.web.model.AuditNode;

/**
 * 
* @ClassName: AuditNodeService 
* @Description: 审核节点Service 
* @author wenquan
* @date 2016年8月8日 下午4:44:10 
*
 */
public interface AuditNodeService extends GenericService<AuditNode, String>{

	/**
	 * 
	* @Title: selectPendent 
	* @Description: 获取指定用户的待审节点信息 
	* @param @param userId
	* @param @return    设定文件 
	* @return List<AuditNode>    返回类型 
	* @throws
	 */
	public List<AuditNode> selectPendent(String userId);
	
	/**
	 * 
	* @Title: selectCommittedNode 
	* @Description: 查询已提交的事项节点 
	* @param @param userId
	* @param @return    设定文件 
	* @return List<AuditNode>    返回类型 
	* @throws
	 */
	public List<AuditNode> selectCommittedNode(String userId);
	
	/**
