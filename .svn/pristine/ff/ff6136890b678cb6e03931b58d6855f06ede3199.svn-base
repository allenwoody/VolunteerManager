#admin_header
<div class="am-cf admin-main">
#admin_sidebar
<div class="admin-content" style="min-height:600px">

  <div class="am-cf am-padding">
    <div class="am-fl am-cf"><strong class="am-text-primary am-text-lg">机构管理</strong> / <small>新增</small></div>
  </div>
  	<div class="am-container">
        <form id="frmMain" method="post" class="am-form" action="${rc.contextPath}/organization/insertOrganization.html" data-am-validator>
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
              上级机构
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" disabled="disabled" value="${parentOrg.orgName}" class="am-input-sm" required>
              <input type="hidden" name="parentId" value="${parentOrg.orgId}"/>
            </div>
          </div>
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
              机构名称
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="orgName" class="am-input-sm" required>
            </div>
          </div>

          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
             联系电话
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="telephone" class="am-input-sm" required>
            </div>
          </div>
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
            地址
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="address" class="am-input-sm" required>
            </div>
          </div>
          
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
              机构类型
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <select name="orgType" placeholder="点击选择.." data-am-selected required>
              	 <option selected value=""></option>
              	#foreach($orgType in $orgTypeMap.entrySet())
				  <option value="$!{orgType.key}">$!{orgType.value}</option>
				#end
			  </select>
            </div>
          </div>
          
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
            开户行
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="bankName" class="am-input-sm" >
            </div>
          </div>
          
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
             开户行账号
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="bankAccount" class="am-input-sm">
            </div>
          </div>
          
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
             财务负责人
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="financialChief" class="am-input-sm">
            </div>
          </div>
          
          <div class="am-g am-margin-top">
            <div class="am-u-sm-4 am-u-md-2 am-text-right">
             财务负责人电话
            </div>
            <div class="am-u-sm-8 am-u-md-4 am-u-end">
              <input type="text" name="financialPhone" class="am-input-sm">
            </div>
          </div>
          
        </form>
      </div>
   <div class="am-margin">
    <button type="button" id="submitBtn" class="am-btn am-btn-primary am-btn-xs">提交保存</button>
    <button type="button" id="cancelBtn" class="am-btn am-btn-primary am-btn-xs">放弃保存</button>
  </div>
</div>
</div>
#admin_sidebar_sm
#admin_footer

<script type="text/javascript">
$(function(){
	$("#submitBtn").click(function(){
		$("#frmMain").submit();
	});
	$("#cancelBtn").click(function(){
		window.location.href='${rc.contextPath}/organization/listOrganization.html';
	});
	
});
</script>